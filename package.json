{
	"name": "vscode-configlens",
	"displayName": "Config Lens",
	"description": "Improved visibility and controls of shared global settings in Visual Studio Code",
	"publisher": "sebster",
	"license": "MIT",
	"version": "0.0.1",
	"preview": true,
	"icon": "images/logo.png",
	"categories": [
		"Other"
	],
	"keywords": [
		"settings.json",
		"shared settings",
		"applyToAllProfiles"
	],
	"engines": {
		"vscode": ">=1.83.0"
	},
	"main": "./out/extension",
	"files": [
		"images",
		"out"
	],
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"lint": "eslint src/**/*.ts",
		"format": "prettier src/**/*.ts --write",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.5",
		"typescript": "^5.3.2"
	},
	"homepage": "https://github.com/Sebsterio/vscode-configlens",
	"repository": {
		"type": "git",
		"url": "https://github.com/Sebsterio/vscode-configlens"
	},
	"bugs": {
		"url": "https://github.com/Sebsterio/vscode-configlens/issues"
	},
	"author": {
		"name": "Sebastian Rosloniec",
		"url": "https://sebster.dev",
		"email": "sebster.main@gmail.com"
	}
}
